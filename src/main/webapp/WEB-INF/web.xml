  	<servlet-name>LocataireServlet</servlet-name>
  	<url-pattern>/supprLocataire</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>LocataireServlet</servlet-name>
  	<url-pattern>/ajouterLocataire</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet-mapping>
  	<servlet-name>LouerServlet</servlet-name>
  	<url-pattern>/listerLouer</url-pattern>
