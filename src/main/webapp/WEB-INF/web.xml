  	<servlet-name>LouerServlet</servlet-name>
  	<url-pattern>/supprLouer</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>LouerServlet</servlet-name>
  	<url-pattern>/ajouterLouer</url-pattern>
