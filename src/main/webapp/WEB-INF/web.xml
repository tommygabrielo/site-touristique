  	<servlet-name>LouerServlet</servlet-name>
  	<url-pattern>/modifLouer</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>LouerServlet</servlet-name>
  	<url-pattern>/editerLouer</url-pattern>
