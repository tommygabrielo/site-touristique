<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>site</display-name>
  <welcome-file-list>
    <welcome-file></welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<description></description>
  	<display-name>LoginServlet</display-name>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>net.site.controller.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<description></description>
  	<display-name>SiteServlet</display-name>
  	<servlet-name>SiteServlet</servlet-name>
  	<servlet-class>net.site.controller.SiteServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<description></description>
  	<display-name>LocataireServlet</display-name>
  	<servlet-name>LocataireServlet</servlet-name>
  	<servlet-class>net.site.controller.LocataireServlet</servlet-class>
  </servlet>
  
  <servlet>
