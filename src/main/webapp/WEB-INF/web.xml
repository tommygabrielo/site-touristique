  	<display-name>LouerServlet</display-name>
  	<servlet-name>LouerServlet</servlet-name>
  	<servlet-class>net.site.controller.LouerServlet</servlet-class>
  </servlet>
  
  
 
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/listerSite</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/listerRechercher</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/modifSite</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/editerSite</url-pattern>
  </servlet-mapping>
  
   <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/supprSite</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>SiteServlet</servlet-name>
  	<url-pattern>/ajouterSite</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet-mapping>
  	<servlet-name>LocataireServlet</servlet-name>
  	<url-pattern>/listerLocataire</url-pattern>
